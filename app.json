{
  "expo": {
    "name": "Финансы",
    "slug": "personal-finance-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pfinance",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#1b1b1d",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b1b1d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1b1b1d"
      },
      "package": "ru.tyuop.financeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fb35713a-5db3-4f4b-85ee-50ebb5c13ab9"
      }
    },
    "owner": "tyuop077"
  }
}
